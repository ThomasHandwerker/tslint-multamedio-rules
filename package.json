{
  "name": "tslint-multamedio-rules",
  "version": "1.0.2",
  "description": "Custom rule implementations for tslint, in order to fullfill typescript style guide of MULTA MEDIO AG",
  "scripts": {
    "compile": "tsc -p src/tsconfig.json",
    "postinstall": "./bin/postinstaller.sh",
    "postuninstall": "./bin/postuninstaller.sh",
    "test": "node build/test/ruleTestRunner.js",
    "watch": "nodemon -w src -w test -e \"ts,json,lint\" -x \"npm-run-all -p compile test\""
  },
  "repository": {
    "type": "git",
    "url": "https://gitlab.informagic.net/thandwerker/tslint-multamedio-rules.git"
  },
  "keywords": [
    "linter",
    "typescript",
    "custom-rules"
  ],
  "author": "Thomas Handwerker <thandwerker@multamedio.de>",
  "license": "Apache-2.0",
  "devDependencies": {
    "nodemon": "^1.11.0",
    "npm-run-all": "^3.1.1",
    "tslint": "3.15.1",
    "typescript": "1.8.x"
  },
  "peerDependencies": {
    "tslint": "3.15.1"
  }
}
